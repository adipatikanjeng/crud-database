{
    "name": "adipatikanjeng/crud-database",
    "description": "A Laravel Admin Package to make your life easier and steer your project in the right direction",
    "keywords": ["laravel", "admin", "panel"],
    "license": "MIT",
    "homepage": "http://laraveladminpanel.com",
    "support": {
        "issues": "http://laraveladminpanel.com/issues",
        "demo": "http://demo.laraveladminpanel.com/admin"
    },
    "authors": [
        {
            "name": "Heriyadi",
            "email": "adipatikanjeng@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "intervention/image": "^2.4",
        "doctrine/dbal": "^2.5",
        "larapack/doctrine-support": "~0.1.4",
        "arrilot/laravel-widgets": "^3.7",
        "league/flysystem": "~1.0.41"
    },
    "require-dev": {
        "phpunit/phpcov": "~4.0|~5.0|~6.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0|~8.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0|~5.8.0",
        "orchestra/testbench": "~3.0",
        "laravel/browser-kit-testing": "~2.0.0|~3.0.0|~4.0.0|~5.0.0|~5.1",
        "orchestra/testbench-browser-kit": "~3.5|~3.6|~3.7|~3.8",
        "symfony/css-selector": "3.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelAdminPanel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelAdminPanel\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "beta",
    "extra": {
        "laravel": {
            "providers": [
                "LaravelAdminPanel\\AdminServiceProvider"
            ]
        }
    }
}
